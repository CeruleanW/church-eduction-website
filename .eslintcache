mponents/NavTabs.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/components/Nav.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/components/Footer/Footer.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/components/SimpleMenu.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/components/Cards/CardsPageBase.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/components/Footer/FooterContent.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/components/MainGridContainer.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/components/NewsFeedPage/NewsItem.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/components/NewsFeedPage/Filter.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/components/Intro/Title.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/components/Intro/Description.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/components/Intro/Intro.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/components/Overview.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/components/NewsFeedPage/LinkArea.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/components/Cards/CardGrids.jsx",["160"],"import React from 'react';\r\nimport CustomCard from './CustomCard';\r\nimport { Grid } from '@material-ui/core';\r\nimport { AppContextProvider } from '../AppContext';\r\nimport ExpandablePaper from './ExpandablePaper';\r\n\r\nfunction CardGridList(props) {\r\n  return (\r\n    <>\r\n      {props.items.map((data, index) => (\r\n        <Grid item xs={12} md={6} lg={4} xl={3} key={'card-grid-' + index}>\r\n          <CustomCard key={'card-' + index} {...data} />\r\n        </Grid>\r\n      ))}\r\n    </>\r\n  );\r\n}\r\n\r\nexport default function CardGrids(props) {\r\n  const { cardList, itemType } = props;\r\n  let titleToCoursesMap;\r\n  let titlesArray;\r\n  let renderedCards;\r\n\r\n  if (cardList) {\r\n    titleToCoursesMap = generateTitleToCoursesMap(cardList); //key: title, value: course_data\r\n    // const titleToCoursesMap = generateTitleToCoursesMap(terms); //key: title, value: course_data\r\n    titlesArray = [...titleToCoursesMap.keys()]; // an array of existing titles\r\n    renderedCards = titlesArray.map((title, index) => (\r\n      <Grid container item xs={12} key={'grid-panel' + index}>\r\n        <ExpandablePaper label={title} key={'expandable-' + index}>\r\n          <CardGridList items={titleToCoursesMap.get(title)} />\r\n        </ExpandablePaper>\r\n      </Grid>\r\n    ));\r\n  }\r\n\r\n  return (\r\n    <>\r\n      <AppContextProvider>\r\n        {renderedCards !== undefined ? renderedCards : null}\r\n      </AppContextProvider>\r\n    </>\r\n  );\r\n}\r\n\r\n//return a map contains courses for each term, so we can sort courses by the term\r\nfunction generateTitleToCoursesMap(cardList) {\r\n  const terms = cardList.map((data) => data.term);\r\n  let titleMapObject = new Map();\r\n  for (const term of terms) {\r\n    let courseList = term.courses;\r\n    let preCourseTitle = courseList[0].title;\r\n    let courseSerie = []; // all classes in the same title\r\n    for (const course of courseList) {\r\n      if (course.title === preCourseTitle) {\r\n        // add this course to courseSerie\r\n        courseSerie.push(course);\r\n      } else {\r\n        // put courseSerie in the map\r\n        titleMapObject.set(course.title, courseSerie);\r\n        courseSerie = [];\r\n        preCourseTitle = course.title;\r\n      }\r\n      titleMapObject.set(course.title, courseSerie);\r\n    }\r\n  }\r\n  return titleMapObject;\r\n}\r\n","/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/components/NewsFeedPage/LinkItem.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/components/Cards/ExpandablePaper.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/components/AppContext.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/components/Cards/CustomCard.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/components/ItemPage/Description.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/components/ItemPage/Share.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/components/ItemPage/Title.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/components/ItemPage/Resources/Resources.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/components/ItemPage/Resources/File.jsx",[],["161","162"],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/components/TopicGroup.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/components/Video/Video.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/components/Video/VideoSkeleton.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/lib/loadData.js",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/pages/NewsFeedPage.jsx",["163"],"import React, { useState, useRef, useEffect } from 'react';\r\nimport NewsItem from '../components/NewsFeedPage/NewsItem';\r\nimport Intro from '../components/Intro/Intro';\r\nimport { Grid, Box, Link, Typography, LinearProgress } from '@material-ui/core';\r\nimport Filter from '../components/NewsFeedPage/Filter';\r\nimport MainGridContainer from '../components/MainGridContainer';\r\nimport Title from '../components/Intro/Title';\r\nimport Description from '../components/Intro/Description';\r\nimport { readNews, fetchGet } from '../lib/loadData';\r\n\r\n// get a set of existing terms in the data, e.g. [2010, 2011, 2012]\r\nfunction getTermSet(newsList) {\r\n  const set = new Set();\r\n  for (const item of newsList) {\r\n    set.add(item.term);\r\n  }\r\n  return set;\r\n}\r\n\r\n//TODO: refactor to an array later\r\nfunction getGroupNames(newsList) {\r\n  return newsList.name;\r\n}\r\n\r\nfunction renderNews(items) {\r\n  return (\r\n    <>\r\n      {items.map((news, i) => (\r\n        <Grid key={'news-grid-' + i} item xs={12}>\r\n          <NewsItem key={'news-' + i} {...news} />\r\n        </Grid>\r\n      ))}\r\n    </>\r\n  );\r\n}\r\n\r\n// get the news list for a specific term, return all news list if term is false\r\nfunction filterByTerm(newsList, term) {\r\n  let result = newsList;\r\n  if (term) {\r\n    result = result.filter((item) => item.term === term);\r\n  }\r\n  return result;\r\n}\r\n\r\nconst contactEmail = 'mailto:ce@zioncma.ca';\r\n\r\nexport default function NewsFeedPage(props) {\r\n  const { pageTitle } = props;\r\n  const [term, setTerm] = useState('');\r\n  const [isLoading, setIsLoading] = useState(true);\r\n\r\n  const dataRef = useRef(null);\r\n  const newsList = useRef(null); // convert data format to the news list which is ready to populate\r\n\r\n  useEffect(() => {\r\n    const fetchData = async () => {\r\n      dataRef.current = await readNews();\r\n      newsList.current = dataRef.current.news;\r\n      setIsLoading(false);\r\n    };\r\n\r\n    fetchData().catch( (err) => console.error(err));\r\n  }, []);\r\n\r\n  return (\r\n    <>\r\n      {isLoading ? (\r\n        <LinearProgress color={'secondary'} />\r\n      ) : (\r\n        <>\r\n          <Intro>\r\n            <Title text={pageTitle} />\r\n            <Description>\r\n              歡迎來到宣道會錫安堂基教部的網頁。在這裏你可以得到有關主日學的最新消息，下載和重温過去的主日學。如對錫安堂的基督教教育有任何意見，歡迎通過\r\n              ce@zioncma.ca{' '}\r\n              <Link href={contactEmail} style={{ color: 'blue' }}>\r\n                聯絡我們\r\n              </Link>\r\n              。\r\n            </Description>\r\n          </Intro>\r\n          <MainGridContainer>\r\n            <Box display='flex' justifyContent='flex-end' width={'100%'}>\r\n              <Filter\r\n                itemSet={getTermSet(newsList.current)}\r\n                updateTerm={(value) => setTerm(value)}\r\n                currentTerm={term}\r\n              />\r\n            </Box>\r\n            {/* Temp: Use a single group heading as 二零二一年春季主日學 */}\r\n            <Typography variant={'h2'}>{getGroupNames(newsList.current)}</Typography>\r\n            {renderNews(filterByTerm(newsList.current, term))}\r\n          </MainGridContainer>\r\n        </>\r\n      )}\r\n    </>\r\n  );\r\n}\r\n","/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/pages/NoMatchPage.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/pages/GrowthPage.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/pages/EducationPage.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/pages/ArchivePage.jsx",[],"/mnt/44C23595C2358C64/MyEntryPoint/MyCode/Projects/cew/src/pages/ItemPage.jsx",[],{"ruleId":"164","replacedBy":"165"},{"ruleId":"166","replacedBy":"167"},{"ruleId":"168","severity":1,"message":"169","line":20,"column":21,"nodeType":"170","messageId":"171","endLine":20,"endColumn":29},{"ruleId":"164","replacedBy":"172"},{"ruleId":"166","replacedBy":"173"},{"ruleId":"168","severity":1,"message":"174","line":9,"column":20,"nodeType":"170","messageId":"171","endLine":9,"endColumn":28},"no-native-reassign",["175"],"no-negated-in-lhs",["176"],"no-unused-vars","'itemType' is assigned a value but never used.","Identifier","unusedVar",["175"],["176"],"'fetchGet' is defined but never used.","no-global-assign","no-unsafe-negation"]