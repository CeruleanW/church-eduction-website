urn null;\r\n  // }\r\n\r\n  return (\r\n    <>\r\n      <AppContextProvider>\r\n        {titlesArray.map((title, index) => (\r\n          <Grid container item xs={12} key={\"grid-panel\" + index}>\r\n            <ExpandablePaper label={title} key={\"expandable-\" + index}>\r\n              <CardGridList items={titleToCoursesMap.get(title)} />{\" \"}\r\n            </ExpandablePaper>\r\n          </Grid>\r\n        ))}\r\n      </AppContextProvider>\r\n    </>\r\n  );\r\n}\r\n\r\nfunction generateTitleToCoursesMap(terms) {\r\n  let titleMapObject = new Map();\r\n  for (const term of terms) {\r\n    let courseList = term.courses;\r\n    let preCourseTitle = courseList[0].title;\r\n    let courseSerie = []; // all classes in the same title\r\n    for (const course of courseList) {\r\n      if (course.title === preCourseTitle) {\r\n        // add this course to courseSerie\r\n        courseSerie.push(course);\r\n      } else {\r\n        // put courseSerie in the map\r\n        titleMapObject.set(course.title, courseSerie);\r\n        courseSerie = [];\r\n        preCourseTitle = course.title;\r\n      }\r\n      titleMapObject.set(course.title, courseSerie);\r\n    }\r\n  }\r\n  return titleMapObject;\r\n}\r\n","E:\\MyEntryPoint\\MyCode\\Projects\\cew\\src\\components\\Cards\\CustomCard.jsx",[],"E:\\MyEntryPoint\\MyCode\\Projects\\cew\\src\\components\\ItemPage\\ItemPage.jsx",[],"E:\\MyEntryPoint\\MyCode\\Projects\\cew\\src\\components\\ItemPage\\Title.jsx",[],"E:\\MyEntryPoint\\MyCode\\Projects\\cew\\src\\components\\ItemPage\\Share.jsx",[],"E:\\MyEntryPoint\\MyCode\\Projects\\cew\\src\\components\\ItemPage\\Video.jsx",[],"E:\\MyEntryPoint\\MyCode\\Projects\\cew\\src\\components\\ItemPage\\Description.jsx",[],"E:\\MyEntryPoint\\MyCode\\Projects\\cew\\src\\components\\ItemPage\\Resources\\Resources.jsx",[],"E:\\MyEntryPoint\\MyCode\\Projects\\cew\\src\\components\\ItemPage\\Resources\\File.jsx",[],"E:\\MyEntryPoint\\MyCode\\Projects\\cew\\src\\components\\AppContext.j